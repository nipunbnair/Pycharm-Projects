    "#2\n",
    "def generateTheoreticalSpectrum(string):\n",
    "    temp=permutate_all_substrings(string)\n",
    "    Spectrum=[]\n",
    "    for i in temp:\n",
    "        Spectrum.append([getAminoWeight(i),i])\n",
    "        \n",
    "    return Spectrum\n",
    "\n",
    "TempResult=generateTheoreticalSpectrum(\"NQEL\")\n",
    "\n",
    "result=[[0,\"\"]]+TempResult\n",
    "result"
