    "    while j<m:\n",
    "        if P[j]==T[i+j]:\n",
    "            j=j+1\n",
    "            \n",
    "        else: \n",
    "            break    \n",
    "    if j==m:\n",
    "           print(\"Pattern found at\",i)\n",
